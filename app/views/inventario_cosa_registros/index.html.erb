<p id="notice"><%= notice %></p>

<h1>Listing Inventario Cosa Registros</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Inventario</th>
      <th>Cosa</th>
      <th>Tipo</th>
      <th>Cantidad</th>
      <th>Notas</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inventario_cosa_registros.each do |inventario_cosa_registro| %>
      <tr>
        <td><%= inventario_cosa_registro.inventario_cosa.inventario %></td>
        <td><%= inventario_cosa_registro.inventario_cosa.cosa.nombre %></td>
        <td><%= inventario_cosa_registro.tipo %></td>
        <td><%= inventario_cosa_registro.cantidad %></td>
        <td><%= inventario_cosa_registro.notas %></td>
        <td><%= link_to 'Show', inventario_cosa_registro %></td>
        <td><%= link_to 'Edit', edit_inventario_cosa_registro_path(inventario_cosa_registro) %></td>
        <td><%= link_to 'Destroy', inventario_cosa_registro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inventario cosa registro', new_inventario_cosa_registro_path %>
