<p id="notice"><%= notice %></p>

<h1>Listing Empaques</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @empaques.each do |empaque| %>
      <tr>
        <td><%= empaque.nombre %></td>
        <td><%= empaque.url %></td>
        <td><%= link_to 'Show', empaque %></td>
        <td><%= link_to 'Edit', edit_empaque_path(empaque) %></td>
        <td><%= link_to 'Destroy', empaque, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Empaque', new_empaque_path %>
